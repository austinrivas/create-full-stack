{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "apollo-boost": "^0.4.2",
    "apollo-client": "^2.6.2",
    "apollo-link-context": "^1.0.18",
    "dotenv": "^8.0.0",
    "firebase": "7.9.0",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "react": "^16.7.0",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.7.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "typescript": "3.4.5",
    "use-react-router": "^1.0.7"
  },
  "scripts": {
    "start": "react-scripts --require dotenv/config start",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Generate,Start\" -c \"magenta.bold,cyan.bold\" \"yarn generate\" \"yarn start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen --config codegen.yml --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "^1.2.1",
    "@graphql-codegen/typescript": "^1.2.1",
    "@graphql-codegen/typescript-operations": "^1.2.1",
    "@graphql-codegen/typescript-react-apollo": "^1.2.1",
    "@types/react": "^16.8.20",
    "@types/react-router-dom": "^4.3.4",
    "concurrently": "^4.1.0",
    "husky": "^2.4.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
